{
  "name": "jit-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "sh -ac '. ./.env.$npm_config_env; vite'",
    "build": "sh -ac '. ./.env.$npm_config_env; tsc && vite build'",
    "serve": "sh -ac '. ./.env.$npm_config_env; tsc && vite preview'",
    "test": "NODE_ENV=test jest --config=jest.config.js",
    "test:ci-pages": "NODE_ENV=test node --expose-gc ./node_modules/.bin/jest --config=jest.config.js --logHeapUsage --coverage --silent ----testPathPattern=src/pages",
    "test:ci-core": "NODE_ENV=test node --expose-gc ./node_modules/.bin/jest --config=jest.config.js --logHeapUsage --coverage --silent ----testPathIgnorePatterns=src/pages",
    "lint": "eslint src --max-warnings=0",
    "lint:staged": "lint-staged",
    "deploy": "npm run build --env=$npm_config_env && npm run deploy-no-build --env=$npm_config_env",
    "deploy-no-build": "cd ./build && aws-vault exec jit-$npm_config_env-admin -- aws s3 sync . s3://platform.$npm_config_env.jitdev.io --delete && KEY_ID=$(aws-vault exec jit-$npm_config_env-admin -- aws cloudfront list-distributions | jq -r '.DistributionList.Items[].Id') && aws-vault exec jit-$npm_config_env-admin -- aws cloudfront create-invalidation --distribution-id $KEY_ID --paths '/*'",
    "analyze": "source-map-explorer 'build/*.js' --json bundle_analyze.json --no-border-checks && source-map-explorer 'build/*.js' --html bundle_analyze.html --no-border-checks",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "find-deadcode": "ts-prune -s | grep -v -e 'src/mocks' -e 'src/utils/testUtils' -e '(used in module)' | (! grep -v -e 'index.ts' -e 'stories.tsx' -e 'cypress.config.ts' -e 'playwright.config.ts' -e 'GithubContext.tsx' -e 'JitVolume' -e 'useAegisService.ts' -e 'drilldown.ts' -e 'useSendMessage.ts' -e 'useGetAgents.ts' -e 'Header.tsx' -e 'promptsData.ts' -e 'styles/styled.ts')",
    "prepare": "husky",
    "playwright:ci": "sh -ac '. ./.env.$npm_config_env; playwright test'",
    "playwright:open": "sh -ac '. ./.env.$npm_config_env; playwright test --ui'",
    "playwright:debug": "sh -ac '. ./.env.$npm_config_env; PWDEBUG=1 playwright test $npm_config_file'",
    "generate-tokens": "node scripts/generate-design-tokens.scss.js"
  },
  "dependencies": {
    "@atlaskit/adf-schema": "^25.3.1",
    "@atlaskit/adf-utils": "^18.0.3",
    "@frontegg/react": "^7.5.0",
    "@jest/globals": "^29.7.0",
    "@monaco-editor/react": "^4.5.1",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.3",
    "@segment/analytics-next": "^1.17.4",
    "@sentry/react": "^7.28.1",
    "@sentry/tracing": "^7.28.1",
    "@stigg/react-sdk": "^5.26.0",
    "@testing-library/react": "^16.0.1",
    "@useparagon/connect": "^2.0.8",
    "ajv-formats": "^2.1.1",
    "axios": "^0.24.0",
    "cronstrue": "^3.2.0",
    "deep-diff": "^1.0.2",
    "elkjs": "^0.9.3",
    "file-saver": "^2.0.5",
    "free-email-domains": "^1.0.28",
    "html-react-parser": "^5.2.5",
    "html2canvas": "^1.4.1",
    "i18next": "^21.5.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "jest-canvas-mock": "^2.5.2",
    "jest-junit": "^16.0.0",
    "js-yaml": "^4.1.0",
    "json2md": "^1.12.0",
    "jspdf": "^3.0.2",
    "launchdarkly-react-client-sdk": "^2.29.4",
    "lodash": "^4.17.21",
    "lottie-react": "^2.4.0",
    "markdown-to-jsx": "^7.1.7",
    "mermaid": "^11.9.0",
    "mixpanel-browser": "^2.45.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.14.2",
    "react-mixpanel": "^1.0.5",
    "react-query": "^3.39.3",
    "react-resizable-panels": "^3.0.4",
    "react-resize-detector": "^7.1.1",
    "react-router-dom": "^6.0.2",
    "react-table": "^7.8.0",
    "react-toastify": "^11.0.2",
    "react-use-hotjar": "^1.2.5",
    "react-usestateref": "^1.0.8",
    "reactflow": "^11.11.3",
    "recharts": "^2.4.3",
    "use-query-params": "^2.1.0",
    "uuid": "^8.3.2",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@frontegg/types": "^7.54.0",
    "@playwright/test": "^1.50.1",
    "@sentry/vite-plugin": "^2.10.0",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/client-api": "^7.6.17",
